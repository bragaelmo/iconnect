<ul id="messages"></ul>
<script>
  fetch('http://localhost:8080/contacts')
  .then((response) =>response.json())
  .then(data => {
    const list = document.querySelector('#messages');
    data.map((item) => {
        const li = document.createElement('li');
        li.setAttribute('id',item.id);
        li.innerHTML = item.name;
        list.appendChild(li);
    })
    console.log(data);
  })
</script>